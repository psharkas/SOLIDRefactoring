import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class Tests {
	
	@Test
	void testDVDInstance() {
		DVD dvd1 = new DVD();
		assertNotNull(dvd1);
	}
	
	@Test
	void testDVDTitle() {
		Movie dvd2 = new DVD();
		String expected = "Avatar the Last Airbender";
		dvd2.setTitle("Avatar the Last Airbender");
		String actual = dvd2.getTitle();
		assertEquals(expected,actual);
	}
	
	@Test
	void testDVDRunTime() {
		Movie dvd3 = new DVD();
		int expected = 103;
		dvd3.setRunTime(103);
		int actual = dvd3.getRunTime();
		assertEquals(expected,actual);
	}
	
	@Test
	void testVHSInstance() {
		Movie vhs1 = new VHS();
		assertNotNull(vhs1);
	}
	
	@Test
	void testVHSTitle() {
		Movie vhs2 = new VHS();
		String expected = "Avatar the Last Airbender";
		vhs2.setTitle("Avatar the Last Airbender");
		String actual = vhs2.getTitle();
		assertEquals(expected,actual);
	}
	
	@Test
	void testVHSRunTime() {
		Movie vhs3 = new VHS();
		int expected = 103;
		vhs3.setRunTime(103);
		int actual = vhs3.getRunTime();
		assertEquals(expected,actual);
	}
	
//	@Test
//	void testVHSCurrentTime() {
//		Movie vhs4 = new VHS();
//		int expected = 0;
//		int actual = vhs4.getCurrentTime();
//		assertEquals(expected,actual);
//	}


}
