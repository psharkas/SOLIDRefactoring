import java.util.ArrayList;

public class MovieApp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ArrayList<Movie> movies = new ArrayList<>();
		Movie vhs1 = new VHS();
		Movie vhs2 = new VHS();
		Movie vhs3 = new VHS();
		Movie dvd1 = new DVD();
		Movie dvd2 = new DVD();
		Movie dvd3 = new DVD();

		movies.add(vhs1);
		movies.add(vhs2);
		movies.add(vhs3);
		movies.add(dvd1);
		movies.add(dvd2);
		movies.add(dvd3);

		vhs1.printInfo();
}
}